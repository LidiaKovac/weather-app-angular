{
  "name": "weather-app-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently \"npm run config && ng serve -o\" \"npm run backend\"",
    "start": "npm run config && ng build --configuration production && ng s --configuration production",
    "build": "npm run config && ng build",
    "watch": "ng build --watch --configuration development",
    "backend": "json-server-auth --watch db.json --port 3000",
    "test": "ng test",
    "config": "ts-node src/environments/writeEnv.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@auth0/angular-jwt": "^5.1.2",
    "bootstrap-icons": "^1.10.5",
    "dotenv": "^16.3.1",
    "json-server": "^0.17.3",
    "json-server-auth": "^2.1.0",
    "moment": "^2.29.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "~14.2.11",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
